@model Diplom.Models.VaultNote



<h3>Вы уверены что хотите удалить?</h3>
<div>
    <h4>Запись свода</h4>
    <hr />
    <dl class="row">
        
        <dt class = "col-sm-2">
            Дата
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Date)
        </dd>

    </dl>
    
    <form asp-action="Delete">
        <input type="hidden" asp-for="Id" />
        <button type="submit" name="IdVault" value="@Model.IdVault" class="btn btn-danger">Удалить</button>
        <a asp-action="Index" class="btn" asp-route-idVault="@Model.IdVault"><i class="bi bi-arrow-return-left"></i>Вернуться</a>
    </form>
</div>

